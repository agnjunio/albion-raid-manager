{
  "name": "@albion-raid-manager/bot",
  "version": "1.0.0",
  "description": "Discord bot for Albion Raid Manager.",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx .",
    "dev": "tsx watch --env-file=.env --exclude=logs/** --exclude=src/**/*.test.ts .",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier -c src/",
    "prettier:fix": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@albion-raid-manager/ai": "workspace:*",
    "@albion-raid-manager/albion": "workspace:*",
    "@albion-raid-manager/core": "workspace:*",
    "@albion-raid-manager/database": "workspace:^",
    "@albion-raid-manager/redis": "workspace:*",
    "@albion-raid-manager/types": "workspace:*",
    "discord.js": "^14.19.3"
  }
}

{
  "name": "@albion-raid-manager/core",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "require": "./src/*.ts"
    },
    "./cache/*": {
      "types": "./src/cache/*/index.ts",
      "import": "./src/cache/*/index.ts",
      "require": "./src/cache/*/index.ts"
    },
    "./scheduler": {
      "types": "./src/scheduler.ts",
      "import": "./src/scheduler.ts",
      "require": "./src/scheduler.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts",
      "require": "./src/utils/index.ts"
    },
    "./entities": {
      "types": "./src/entities/index.ts",
      "import": "./src/entities/index.ts",
      "require": "./src/entities/index.ts"
    },
    "./services": {
      "types": "./src/services/index.ts",
      "import": "./src/services/index.ts",
      "require": "./src/services/index.ts"
    },
    "./config": {
      "types": "./src/config/index.ts",
      "import": "./src/config/index.ts",
      "require": "./src/config/index.ts"
    },
    "./logger": {
      "types": "./src/logger/index.ts",
      "import": "./src/logger/index.ts",
      "require": "./src/logger/index.ts"
    },
    "./database": {
      "types": "./src/database/index.ts",
      "import": "./src/database/index.ts",
      "require": "./src/database/index.ts"
    }
  },
  "scripts": {
    "dev": "echo 'JIT compilation - no build needed'",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@albion-raid-manager/types": "workspace:*",
    "@albion-raid-manager/redis": "workspace:*",
    "axios": "^1.8.4",
    "cron": "^3.1.7",
    "discord-api-types": "^0.37.69",
    "winston": "^3.13.0",
    "zod": "^3.23.8",
    "@prisma/client": "6.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "prisma": "6.13.0",
    "prisma-generator-typescript-interfaces": "^2.1.0",
    "typescript": "^5.3.3"
  }
}

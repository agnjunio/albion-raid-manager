{
  "name": "@albion-raid-manager/core",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "require": "./src/*.ts"
    },
    "./cache/*": {
      "types": "./src/cache/*/index.ts",
      "import": "./src/cache/*/index.ts",
      "require": "./src/cache/*/index.ts"
    },
    "./scheduler": {
      "types": "./src/scheduler.ts",
      "import": "./src/scheduler.ts",
      "require": "./src/scheduler.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts",
      "require": "./src/utils/index.ts"
    },
    "./entities": {
      "types": "./src/entities/index.ts",
      "import": "./src/entities/index.ts",
      "require": "./src/entities/index.ts"
    },
    "./services": {
      "types": "./src/services/index.ts",
      "import": "./src/services/index.ts",
      "require": "./src/services/index.ts"
    }
  },
  "scripts": {
    "dev": "echo 'JIT compilation - no build needed'",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@albion-raid-manager/config": "workspace:^",
    "@albion-raid-manager/database": "workspace:*",
    "@albion-raid-manager/logger": "workspace:*",
    "@albion-raid-manager/types": "workspace:*",
    "@albion-raid-manager/redis": "workspace:*",
    "@albion-raid-manager/discord": "workspace:*",
    "cron": "^3.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}

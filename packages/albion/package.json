{
  "name": "@albion-raid-manager/albion",
  "version": "1.0.0",
  "description": "Albion Online API client for the Albion Raid Manager",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./servers": {
      "types": "./src/servers.ts",
      "import": "./src/servers.ts",
      "require": "./src/servers.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "require": "./src/types.ts"
    }
  },
  "scripts": {
    "dev": "echo 'JIT compilation - no build needed'",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "packageManager": "pnpm@10.6.1",
  "dependencies": {
    "@albion-raid-manager/core": "workspace:^",
    "@albion-raid-manager/config": "workspace:^",
    "@albion-raid-manager/logger": "workspace:^",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
